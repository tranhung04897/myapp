<div class="form-group row">
  <div class="col-sm-6 mb-3 mb-sm-0">
    <%= f.label :name, 'Osi Booker Name', class: 'form-label' %>
    <%= f.text_field :name, autofocus: true, autocomplete: "name", placeholder: "Osi Booker Name", class: 'form-control form-control-user' %>
  </div>
  <div class="col-sm-6">
    <%= f.label :syntax_osi, 'Syntax Osi Booker', class: 'form-label' %>
    <%= f.text_field :syntax_osi, autocomplete: "syntax_osi", placeholder: "Syntax Osi Booker", class: 'form-control form-control-user' %>
  </div>
</div>
<div class="form-group row">
  <div class="col-sm-6 mb-3 mb-sm-0">
    <%= f.label :code, 'Osi Booker Code', class: 'form-label' %>
    <%= f.text_field :code, autofocus: true, autocomplete: "code", placeholder: "Osi Booker Code ", class: 'form-control form-control-user' %>
  </div>
</div>
